{
  "name": "tdg-project",
  "version": "1.0.0",
  "description": "TDG Project for job interview",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "www" : "npm start --prefix www",
